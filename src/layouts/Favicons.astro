---
import computePath from "../helpers/compute-path";
---
<link rel="apple-touch-icon" sizes="180x180" href={computePath("/images/favicons/apple-touch-icon.png")}>
<link rel="icon" type="image/png" href={computePath("/images/favicons/favicon-32x32.png")} sizes="32x32">
<link rel="icon" type="image/png" href={computePath("/images/favicons/favicon-16x16.png")} sizes="16x16">
<link rel="manifest" href={computePath("/images/favicons/manifest.json")}>
<link rel="mask-icon" href={computePath("/images/favicons/safari-pinned-tab.svg")} color="#6e6c65">
<link rel="shortcut icon" href={computePath("/images/favicons/favicon.ico")}>
<meta name="msapplication-config" content="/images/favicons/browserconfig.xml">
<meta name="theme-color" content="#edbf08">