---
import Layout from "../../layouts/Layout.astro"
import data from "../../../data/main.json";

const title = "Programa";
---

<Layout title={title}>
	<div id="content-block">
		<h2>Programa</h2>
		<h3>Sábado { data.tvxs.date.day } de { data.tvxs.date.monthtext }</h3>
		<ul>
			<li><b>14:00 a 15:30</b> Recogida de dorsales { data.tvxs.name } y { data.gtvs.name } en la Asociación de Vecinos de La Nueva.</li>
			<li><b>16:00</b> Salida del { data.tvxs.sequence } { data.tvxs.name } </li>
			<li><b>19:00</b> Cierre de meta { data.tvxs.name }</li>
			<li><b>19:15</b> Entrega Premios { data.tvxs.name }</li>
		</ul> 
		<h3>Domingo { data.tvs.date.day } de { data.tvs.date.monthtext }</h3>
		<ul>
			<li><b>07:00 a 08:30</b> Recogida dorsales { data.tvs.name } en la Asociación de Vecinos de La Nueva.</li>
			<li><b>09:00</b> Salida { data.tvs.sequence } { data.tvs.name }</li>
			<li><b>14:00</b> Cierre de meta { data.tvs.name }</li>
			<li><b>14:15</b> Entrega de premios { data.tvs.name } y { data.gtvs.name }</li>
		</ul>
		<img src="/images/carrera/programa/salida.jpg" alt="Salida de la prueba" />		
	</div>
</Layout>