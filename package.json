{
  "name": "trailsamuno",
  "type": "module",
  "version": "2.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:css": "sass sass/site.scss public/stylesheets/site.css",
    "build:css-prod": "sass --no-source-map --style=compressed sass/site.scss public/stylesheets/site.css",
    "preview": "astro preview",
    "astro": "astro",
    "build:pro": "npx rollup -c && npm run build:css-prod && npm run build"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "astro": "5.14.1",
    "sass": "1.77.8",
    "typescript": "5.5.3"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "0.4.4",
    "rollup": "4.28.1"
  }
}
